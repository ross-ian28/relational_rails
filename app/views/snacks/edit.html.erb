<%= form_with url: "/snacks/#{@snack.id}", method: :patch, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br></br>

  <%= f.label :ferret_safe %>
  <%= f.text_field :ferret_safe %><br></br>

  <%= f.label :count %>
  <%= f.text_field :count %><br></br>

  <%= f.submit "Update snack" %>
<% end %>
