<%= form_with url: "/ferrets", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br></br>

  <%= f.label :hungry %>
  <%= f.text_field :hungry %><br></br>

  <%= f.label :snack_count %>
  <%= f.text_field :snack_count %><br></br>

  <%= f.submit "Create ferret" %>
<% end %>
